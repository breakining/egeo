<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
            http://www.springframework.org/schema/beans/spring-beans.xsd  
            http://code.alibabatech.com/schema/dubbo  
            http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 提供方应用信息，用于计算依赖关系 -->
	<dubbo:application name="basics-scheduler-service" />

	<!-- 使用zookeeper注册中心暴露服务地址 -->
	<dubbo:registry protocol="zookeeper" address="${cluster1.serverList}" />

	<!-- 用dubbo协议在20880端口暴露服务 -->
	<dubbo:protocol name="dubbo" port="20880" dispatcher="all" threadpool="fixed" threads="200" />
	
	<dubbo:provider filter="user" />
	<!-- read -->
	<dubbo:service interface="com.egeo.basics.user.api.read.SysRoleReadService" ref="sysRoleReadService" />
	
	<dubbo:service interface="com.egeo.basics.user.api.read.SysUserReadService" ref="sysUserReadService" />	
	<!-- write -->
		
	<dubbo:service interface="com.egeo.basics.user.api.write.SysRoleWriteService" ref="sysRoleWriteService" />
	
	<dubbo:service interface="com.egeo.basics.user.api.write.SysUserWriteService" ref="sysUserWriteService" />
	


</beans> 

	