package test;


import java.rmi.RemoteException;

import javax.xml.rpc.ServiceException;

import cn.evun.ime.ws.service.impl.AndonServiceImplServiceLocator;
import cn.evun.ime.ws.service.impl.ProdutionInterfaceServiceImplServiceLocator;

public class ProductionInterface {
	
	public static void main(String[] args) throws RemoteException, ServiceException {
		String content = "<?xml version=\"1.0\" encoding=\"UTF-8\"?><TxControl>"
				+"<docId>2015032615280001</docId>"
				+"<msgCatg>XML</msgCatg>"
				+"<charset>UTF-8</charset>"
				+"<version>1.0</version>"
				+"<docDT>20150326152813</docDT>"
				+"<txData><ItemList>"
				+"<Item>"
				+"<factory>1088</factory>"				
				+"<linecode>ZPLINE01</linecode>"					
				+"<serialno>PT0001</serialno>"					
				+"<scodno>NO_0000000456</scodno>"					
				+"<stno>TEST01</stno>"					
				+"<deviceno>D01</deviceno>"					
				+"<intime>2018-07-10 10:00:00</intime>"					
				+"<outtime>2018-07-10 10:01:00</outtime>"					
				+"<sttype>1</sttype></Item>"				
				+"</ItemList></txData>"
				+"</TxControl>";
		ProdutionInterfaceServiceImplServiceLocator service =  new ProdutionInterfaceServiceImplServiceLocator();
		String result = service.getProdutionInterfaceServiceImplPort().MESZP02(content);
		System.out.println("========================================================");
		System.out.println(result);
		System.out.println("========================================================");
	}

}
